{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "type": "string",
      "identifier": "string",
      "name": "string",
      "projectIdentifier": "projectId",
      "secretManager": "string",
      "description": "string"
    }
  ],
  "required": ["type", "valueType", "name", "identifier", "secretManager", "account"],
  "additionalProperties": false,
  "properties": {
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "examples": ["SecretText", "SecretFile"]
    },
    "valueType": {
      "$id": "#/properties/valueType",
      "type": "string",
      "examples": ["inline", "reference"]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "examples": ["string"]
    },
    "project": {
      "$id": "#/properties/project",
      "type": "string",
      "examples": ["project"]
    },
    "account": {
      "$id": "#/properties/account",
      "type": "string",
      "examples": ["account"]
    },
    "organisation": {
      "$id": "#/properties/organisation",
      "type": "string",
      "examples": ["organisation"]
    },
    "identifier": {
      "$id": "#/properties/identifier",
      "type": "string",
      "examples": ["string"]
    },
    "secretManager": {
      "$id": "#/properties/kmsId",
      "type": "string",
      "examples": ["string"]
    },
    "secretManagerName": {
      "$id": "#/properties/secretManagerName",
      "type": "string",
      "examples": ["string"]
    },
    "draft": {
      "$id": "#/properties/draft",
      "type": "boolean",
      "examples": ["false"]
    },
    "lastUpdatedAt": {
      "$id": "#/properties/draft",
      "type": "number",
      "examples": ["0"]
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "examples": ["string"]
    },
    "tags": {
      "$id": "#/properties/tags",
      "type": "array"
    }
  }
}
