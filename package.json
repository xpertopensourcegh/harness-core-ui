  },
  "engines": {
    "node": ">=14.16.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --rulesdir ./scripts/eslint-rules --resolve-plugins-relative-to",
      "sh scripts/typecheck-staged.sh",
      "prettier --check"
    ],
    "*.scss": [
      "prettier --check"
    ],
    "strings.*.yaml": [
      "yarn strings:check"
    ],
    "webpack.devServerProxy.config.js": [
      "exit 1"
    ],
    "*.i18n.ts": [
      "exit 1"
    ]
  },
  "i18nSettings": {
    "extensionToLanguageMap": {
      "es": [
        "es"
      ],
      "en": [
        "en",
        "en-US",
        "en-IN",
        "en-UK"
      ]
    },
    "commonChunkLimit": "70-pipeline"
  }
