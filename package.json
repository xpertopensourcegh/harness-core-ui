{
  "name": "nextgenui",
  "description": "Harness Inc",
  "version": "0.239.0",
  "author": "Harness Inc",
  "license": "Harness Inc",
  "private": true,
  "homepage": "http://app.harness.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wings-software/nextgenui.git"
  },
  "bugs": {
    "url": "https://github.com/wings-software/nextgenui/issues"
  },
  "keywords": [],
  "scripts": {
    "test": "jest src --silent",
    "test:watch": "jest --watch",
    "lint": "eslint  --rulesdir ./scripts/eslint-rules --ext .ts --ext .tsx src",
    "typecheck": "tsc",
    "strings-check": "node ./scripts/yaml-strings-check.js",
    "clean": "rm -rf dist && rm -rf node_modules/.cache",
    "dev": "NODE_ENV=development webpack serve --progress",
    "watch": "yarn clean && NODE_ENV=development webpack --watch --progress",
    "services": "npm-run-all services:*",
    "services:ti-service": "restful-react import --config restful-react.config.js ti-service",
    "services:portal": "restful-react import --config restful-react.config.js portal",
    "services:cdng": "restful-react import --config restful-react.config.js cd-ng",
    "services:pipelineng": "restful-react import --config restful-react.config.js pipeline-ng",
    "services:logs": "restful-react import --config restful-react.config.js logs",
    "services:resourcegroups": "restful-react import --config restful-react.config.js resourcegroups",
    "services:notifications": "restful-react import --config restful-react.config.js notifications",
    "services:cf": "restful-react import --config restful-react.config.js cf",
    "services:rbac": "restful-react import --config restful-react.config.js rbac",
    "postservices:portal": "prettier --write src/services/portal/index.tsx",
    "postservices:cdng": "prettier --write src/services/cd-ng/index.tsx",
    "postservices:logs": "prettier --write src/services/logs/index.tsx",
    "postservices:pipelineng": "prettier --write src/services/pipeline-ng/index.tsx",
    "postservices:resourcegroups": "prettier --write src/services/resourcegroups/index.tsx",
    "postservices:notifications": "prettier --write src/services/notifications/index.tsx",
    "postservices:rbac": "prettier --write src/services/rbac/index.tsx",
    "services:ci": "restful-react import --config restful-react.config.js ci",
    "services:lw": "restful-react import --config restful-react.config.js lw",
    "postservices:ci": "prettier --write src/services/ci/index.tsx",
    "services:cv": "restful-react import --config restful-react.config.js cv",
    "postservices": "prettier --write src/services/**/*.tsx",
    "build": "yarn clean && GIT_COMMIT=$(git log -1 --pretty=%h) GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) webpack",
    "coverage": "yarn test --coverage",
    "grouped-coverage": "yarn coverage && jest-grouped-coverage -i coverage/coverage-summary.json -c coverage-groups.json",
    "docker": "sh -c 'docker build -t harness/nextgenui:\"$0\" . && docker push harness/nextgenui:\"$0\"'",
    "setup-github-registry": "sh scripts/setup-github-registry.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-graphql-types": "graphql-codegen --config graphql-codegen.yml",
    "strings": "npm-run-all strings:*",
    "strings:check": "node scripts/strings/yamlStringsCheck.mjs",
    "strings:genTypes": "node scripts/strings/generateTypesCli.mjs",
    "strings:loaders": "node scripts/strings/generateStrings.mjs",
    "prepare": "husky install"
  },
