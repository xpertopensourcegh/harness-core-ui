{
  "name": "nextgenui",
  "description": "Harness Inc",
  "version": "0.0.1",
  "author": "Harness Inc",
  "license": "Harness Inc",
  "private": true,
  "homepage": "http://app.harness.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wings-software/nextgenui.git"
  },
  "bugs": {
    "url": "https://github.com/wings-software/nextgenui/issues"
  },
  "keywords": [],
  "scripts": {
    "test": "jest src --silent",
    "lint": "eslint --ext .ts --ext .tsx src",
    "typecheck": "tsc",
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development webpack-dev-server",
    "watch": "yarn clean && NODE_ENV=development webpack --watch",
    "build": "yarn clean && webpack",
    "docker": "sh -c 'docker build -t harness/nextgenui:\"$0\" . && docker push harness/nextgenui:\"$0\"'"
  },
  "dependencies": {
    "@blueprintjs/core": "3.26.1",
    "@blueprintjs/datetime": "3.13.0",
    "@blueprintjs/select": "3.12.3",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.3.1",
    "@wings-software/uikit": "1.0.117",
    "@wings-software/xhr-async": "^2.0.7",
    "classnames": "^2.2.6",
    "clipboard-copy": "^3.1.0",
    "constate": "^1.3.2",
    "formik": "^2.1.4",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.2",
    "react-router-dom": "^5.2.0",
    "secure-web-storage": "^1.0.2",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.3",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/testing-library__react-hooks": "^3.2.0",
    "@types/testing-library__user-event": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@wings-software/css-types-loader": "^1.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/testing-library__react": "^10.0.0",
    "@types/testing-library__dom": "^7.0.0"
  },
  "engines": {
    "node": ">=13.8.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "sh scripts/typecheck-staged.sh",
      "prettier --check"
    ],
    "*.scss": [
      "prettier --check"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./scripts/lint-commit-message.js \"$(cat $HUSKY_GIT_PARAMS)\""
    }
  }
}
