  },
  "resolutions": {
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/testing-library__react": "^10.0.0",
    "@types/testing-library__dom": "^7.0.0"
  },
  "engines": {
    "node": ">=13.8.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --rulesdir ./scripts/eslint-rules",
      "sh scripts/typecheck-staged.sh",
      "prettier --check"
    ],
    "*.scss": [
      "prettier --check"
    ],
    "strings.*.yaml": [
      "node scripts/yaml-strings-check.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./scripts/lint-commit-message.js \"$(cat $HUSKY_GIT_PARAMS)\""
    }
  }
}
